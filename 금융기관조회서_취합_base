        a61 = Range(source_wks.Cells(i61 + 2, 3), source_wks.Cells(i62 - 1, 6)).Value
        a62 = Range(source_wks.Cells(i62 + 2, 3), source_wks.Cells(i71 - 2, 7)).Value
